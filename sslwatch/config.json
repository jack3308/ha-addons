{
  "name": "SSLWatch",
  "slug": "sslwatch",
  "version": "0.3",
  "description": "remote cert fetcher",
  "codenotary": "nra4everdev@proton.me",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "boot": "auto",
  "host_network": false,
  "init": false,
  "map": [
    "ssl:rw"
  ],
  "startup": "application",
  "options": {
    "remote_address": null,
    "subdomain": "fok",
    "private-key": null
  },
  "schema": {
    "remote_address": "str",
    "subdomain": "str",
    "private-key": "password"
  }
}
